{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "wisp",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wisp.py",
            "console": "integratedTerminal"
        },
        {
            "name": "(gdb) dream",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dream",
            "args": [
                "m=wlx00c0ca590ce1",
                "i=wlx00c0ca590ce1",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}